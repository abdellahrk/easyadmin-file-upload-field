{
    "name": "bytescommerce/easy-admin-file-upload-field",
    "description": "Adds a new field to easyAdmin",
    "keywords": ["easyadmin", "easyadmin-bundle","file-upload", "file-upload-field", "easycorp"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/bytes-commerce/easy-admin-file-upload-field",
    "license": "MIT",
    "authors": [
        {
            "name": "Maximilian Graf Schimmelmann",
            "email": "schimmelmann@bytes-commerce.de"
        }
    ],
    "require": {
        "easycorp/easyadmin-bundle": "^4",
        "php": "^8.2",
        "symfony/framework-bundle": "^7.2 || ^7.3 || ^7.4",
        "symfony/http-kernel": "^7.2 || ^7.3 || ^7.4",
        "symfony/config": "^7.2 || ^7.3 || ^7.4",
        "symfony/translation": "^7.2 || ^7.3 || ^7.4",
        "symfony/event-dispatcher": "^7.2 || ^7.3 || ^7.4",
        "symfony/console": "^7.2 || ^7.3 || ^7.4",
        "symfony/dotenv": "^7.2 || ^7.3 || ^7.4",
        "symfony/flex": "^2",
        "symfony/yaml": "^7.2 || ^7.3 || ^7.4",
        "symfony/asset": "^7.2 || ^7.3 || ^7.4"
    },
    "autoload": {
        "psr-4": {
            "BytesCommerce\\FileUploadField\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BytesCommerce\\FileUploadField\\Tests\\": "tests/PhpUnit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.22",
        "rector/rector": "^2.0",
        "phpstan/phpstan": "^2.1"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
